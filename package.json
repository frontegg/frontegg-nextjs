{
  "name": "@frontegg/nextjs",
  "version": "1.0.0",
  "author": "Frontegg LTD",
  "license": "MIT",
  "private": true,
  "scripts": {
    "clean": "lerna run clean && rm -rf ./node_modules",
    "dev": "lerna run build:watch --stream --scope '@frontegg/nextjs'",
    "demo": "lerna run dev --stream --scope '@frontegg/demo-saas'",
    "demo12": "lerna run dev --stream --scope '@frontegg/demo-saas-next12'",
    "build": "lerna run build --stream --scope '@frontegg/nextjs'",
    "test": "jest ./packages/nextjs/test/**",
    "start:mock-server": "node tests/mock-server.js",
    "start:test-demo12": "concurrently \"yarn start:mock-server\" \"yarn demo12\"",
    "test-middleware": "export FRONTEGG_TEST_URL='http://localhost:3001' && yarn build && concurrently -k -s last \"yarn start:mock-server\" \"yarn start:playwrite-test\"",
    "prettier-hook": "prettier --config ./.prettierrc --write .",
    "prettier-check-hook": "prettier --config ./.prettierrc --check ."
  },
  "devDependencies": {
    "@playwright/test": "^1.30.0",
    "concurrently": "^7.6.0",
    "@rollup/plugin-node-resolve": "^8.0.1",
    "@types/history": "^4.7.7",
    "@types/jest": "^29.2.4",
    "@types/node": "^13.9.1",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "eslint": "^8.30.0",
    "eslint-plugin-react": "^7.17.0",
    "express": "^4.18.2",
    "history": "^4.9.0",
    "jest": "^29.3.1",
    "lerna": "6.2.0",
    "path": "^0.12.7",
    "pre-commit": "^1.2.2",
    "prettier": "2.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup": "^2.15.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-typescript2": "^0.27.2",
    "ts-jest": "^29.0.3",
    "tslib": "^2.3.0",
    "tslint": "^6.1.0",
    "typescript": "^4.9.4",
    "webpack": "^5.75.0"
  },
  "nyc": {
    "reporter": [
      "html"
    ]
  },
  "pre-commit": [
    "test",
    "prettier-check-hook"
  ],
  "workspaces": [
    "packages/*"
  ]
}
